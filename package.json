{
  "name": "lol-livegame-client",
  "version": "1.0.0",
  "description": "Library to connect to League of Legends - Game Client API",
  "scripts": {
    "test": "nodemon --exec \"node --loader ts-node/esm\" src/test/TestApp.ts",
    "clean": "node -e \"require('fs').rmSync('dist',{recursive:true,force:true})\"",
    "build": "tsc -p tsconfig.json",
    "prepublishOnly": "npm run build"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^25.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "nodemon": "^3.1.11"
  },
  "dependencies": {
    "undici": "^7.19.2"
  },
  "keywords": [
    "league of legends",
    "league client apis",
    "riot games",
    "liveclientdata"
  ],

  "author": "Zonko",
  "repository": "https://github.com/Zonkodonko/lol-livegame-client.git",

  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [ "dist/**", "README.md"],
  "sideEffects": false
}
